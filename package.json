{
  "name": "jsonsage",
  "version": "1.0.0",
  "description": "智能的JSON处理工作流系统，提供自动化监控、验证和转换功能",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "dev": "babel-node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src __tests__",
    "docs": "jsdoc -c jsdoc.json",
    "prepare": "husky install",
    "prepublishOnly": "npm run test && npm run build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "json",
    "workflow",
    "automation",
    "monitoring",
    "validation",
    "transformation",
    "sage"
  ],
  "author": "hongping1963-source",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hongping1963-source/jsonsage.git"
  },
  "bugs": {
    "url": "https://github.com/hongping1963-source/jsonsage/issues"
  },
  "homepage": "https://github.com/hongping1963-source/jsonsage#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chokidar": "^3.5.3",
    "limiter": "^2.1.0",
    "minimatch": "^5.1.0",
    "pako": "^2.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.0",
    "@babel/core": "^7.22.0",
    "@babel/node": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "docdash": "^2.0.1",
    "eslint": "^8.28.0",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "jsdoc": "^4.0.0"
  },
  "directories": {
    "doc": "docs"
  }
}
